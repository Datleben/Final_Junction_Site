@model Customer
<title>Account Details</title>
<div>
    <h3>@Model.CustomerId</h3>
    <br />
    <h3>@Model.CustomerName</h3>
    <br />
    <h3>@Model.CustomerEmail</h3>
    <br />
    <h3>@Model.CustomerPassword</h3>
    <br />
    <h3>@Model.CustomerAddress</h3>
    <br />
    <h3>@Model.SendEmailNotifications</h3>
    <br />
    <h3>@Model.SendTextNotifications</h3>
</div>