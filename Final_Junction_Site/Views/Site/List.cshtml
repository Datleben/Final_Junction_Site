@{
   
}
@model IEnumerable<Site>

<title>List Sites</title>

@foreach (var p in Model)
{
    <div>
        <h3>@p.SiteName</h3>
        <img src="@p.ThumbnailURL" />
        @p.SiteId
        @p.Description
        <a href="@p.URL">Go to Site</a>
        @* <h4>@p.Price.ToString("c")</h4> *@
    </div>
}